generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  measure_type String
  measure_datetime DateTime
  customer_code String
  measure_value Int 
  measure_uuid String
  confirmed_value Boolean
  image_url String
}